<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="231.0" prefWidth="253.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.zju.gis.sdch.ui.EntityTypeAdd">
    <bottom>
        <HBox prefHeight="46.0" prefWidth="253.0" spacing="80.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="btnConfirm" mnemonicParsing="false" text="确认"/>
                <Button fx:id="btnCancel" mnemonicParsing="false" text="取消"/>
            </children>
            <BorderPane.margin>
                <Insets left="50.0" top="10.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
    <center>
        <GridPane prefHeight="146.0" prefWidth="155.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="75.0" minWidth="10.0" prefWidth="55.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="134.0" minWidth="10.0" prefWidth="134.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="编码">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="tfCode" prefHeight="23.0" prefWidth="116.0" GridPane.columnIndex="1"/>
                <Label text="父级编码" GridPane.rowIndex="1"/>
                <TextField fx:id="tfpCode" prefHeight="23.0" prefWidth="116.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                <Label text="名称" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="10.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="tfName" prefHeight="23.0" prefWidth="116.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </center>
    <top>
        <HBox prefHeight="22.0" prefWidth="253.0" BorderPane.alignment="CENTER"/>
    </top>
    <left>
        <VBox prefHeight="200.0" prefWidth="32.0" BorderPane.alignment="CENTER"/>
    </left>
    <right>
        <VBox prefHeight="156.0" prefWidth="32.0" BorderPane.alignment="CENTER"/>
    </right>
</BorderPane>
