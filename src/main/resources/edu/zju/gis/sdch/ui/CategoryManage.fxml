<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="329.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.zju.gis.sdch.ui.CategoryManage">
    <children>
        <TableView fx:id="CategoryTableview" layoutX="34.0" layoutY="93.0" prefHeight="213.0" prefWidth="297.0">
            <columns>
                <TableColumn fx:id="Categoryid" prefWidth="75.0" text="id"/>
                <TableColumn fx:id="CategorypId" prefWidth="75.0" text="pId"/>
                <TableColumn fx:id="Categorfunc" prefWidth="75.0" text="func"/>
                <TableColumn fx:id="Categorydescription" prefWidth="75.0" text="description"/>
            </columns>
        </TableView>
        <Button fx:id="CategoryAdd" layoutX="47.0" layoutY="42.0" mnemonicParsing="false" onAction="#categoryAdd"
                prefHeight="32.0" prefWidth="55.0" text="添加"/>
        <Button fx:id="CategoryDelete" layoutX="127.0" layoutY="42.0" mnemonicParsing="false" onAction="#categoryDelete"
                prefHeight="32.0" prefWidth="55.0" text="删除"/>
        <Button fx:id="CategorySelect" layoutX="198.0" layoutY="42.0" mnemonicParsing="false" onAction="#categorySelect"
                prefHeight="32.0" prefWidth="55.0" text="查找"/>
        <Button fx:id="CategoryModify" layoutX="269.0" layoutY="42.0" mnemonicParsing="false" onAction="#categoryModify"
                prefHeight="32.0" prefWidth="55.0" text="修改"/>
    </children>
</AnchorPane>
