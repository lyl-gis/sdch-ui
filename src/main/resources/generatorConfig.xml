        <sqlMapGenerator targetPackage="edu.zju.gis.sdch.mapper" targetProject="src/main/resources"/>
        <javaClientGenerator type="XMLMAPPER" targetPackage="edu.zju.gis.sdch.mapper" targetProject="src/main/java"/>
